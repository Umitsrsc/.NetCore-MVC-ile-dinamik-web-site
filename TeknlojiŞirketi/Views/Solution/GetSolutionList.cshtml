@using TeknlojiŞirketi.Concrete
@model List<Solution>
@{
    Layout = null;
}

<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Title</th>
        <th>Description</th>
        <th>Image</th>
    </tr>
   @foreach (var item in Model)
    {
       <tr>
           <th>@item.Id</th>
           <th>@item.Title</th>
           <th>@item.Description</th>
           <th>@item.Image</th>
       </tr>
    }
</table>
<br />
<a href="/Solution/AddSolution" class="btn btn-info">new Solution</a>
